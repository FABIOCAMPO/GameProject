    with open(scores_file_path, 'r') as filin:

        for ligne in filin:
            ligne = ligne.strip()
            numero, liste_scores = ligne.split(";", maxsplit=1)
            liste_temp: list = []
            for i in range(1, len(liste_scores) - 1):
                liste_temp.append(liste_scores[i])
            dict_scores[numero] = liste_temp
